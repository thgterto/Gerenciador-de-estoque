{
  "name": "labcontrol-umv",
  "private": true,
  "version": "1.8.0",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --host --open",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.10.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@vercel/speed-insights": "^1.3.1",
    "apexcharts": "^5.3.6",
    "dexie": "^4.0.1",
    "echarts": "^5.5.0",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.9.0",
    "react-dom": "^18.3.1",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.28.0",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.labcontrol.umv",
    "productName": "LabControl UMV",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
